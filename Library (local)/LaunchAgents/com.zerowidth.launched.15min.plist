<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.zerowidth.launched.15min</string>
	<key>StandardErrorPath</key>
	<string>/Users/Shared/zfssnapshot-15min-err.log</string>
	<key>StandardOutPath</key>
	<string>/Users/Shared/zfssnapshot-15min.log</string>
	<key>ProgramArguments</key>
	<array>
		<string>sh</string>
		<string>/Users/Shared/takeSnapshot.sh</string>
		<string>15min</string>
	</array>
	<key>StartCalendarInterval</key>
	<array>
		<dict>
			<key>Minute</key>
			<integer>0</integer>
		</dict>
		<dict>
			<key>Minute</key>
			<integer>15</integer>
		</dict>
		<dict>
			<key>Minute</key>
			<integer>30</integer>
		</dict>
		<dict>
			<key>Minute</key>
			<integer>45</integer>
		</dict>
	</array>
</dict>
</plist>
