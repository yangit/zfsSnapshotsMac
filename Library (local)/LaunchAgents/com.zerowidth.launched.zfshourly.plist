<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.zerowidth.launched.zfshourly</string>
	<key>StandardErrorPath</key>
	<string>/Users/Shared/zfssnapshotErr.log</string>
	<key>StandardOutPath</key>
	<string>/Users/Shared/zfssnapshot.log</string>
	<key>ProgramArguments</key>
	<array>
		<string>sh</string>
		<string>-c</string>
		<string>/usr/local/bin/zfs snapshot z30/y@hourly-$(date +%Y.%m.%d.%H.%M.%S)</string>
	</array>
	<key>StartCalendarInterval</key>
	<array>
		<dict>
			<key>Minute</key>
			<integer>0</integer>
		</dict>		
	</array>
</dict>
</plist>
