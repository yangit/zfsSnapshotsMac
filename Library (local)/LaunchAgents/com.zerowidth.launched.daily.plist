<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.zerowidth.launched.daily</string>
	<key>StandardErrorPath</key>
	<string>/Users/Shared/zfssnapshot-daily-err.log</string>
	<key>StandardOutPath</key>
	<string>/Users/Shared/zfssnapshot-daily.log</string>
	<key>ProgramArguments</key>
	<array>
		<string>sh</string>
		<string>/Users/Shared/takeSnapshot.sh</string>
		<string>daily</string>
	</array>
	<key>StartCalendarInterval</key>
	<array>
		<dict>
			<key>Hour</key>
			<integer>12</integer>
		</dict>
	</array>
</dict>
</plist>
